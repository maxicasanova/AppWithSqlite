{
  "expo": {
    "name": "my-list-app",
    "slug": "my-list-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": [
      "ios",
      "android"
    ],
    "icon": "./assets/lista.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/lista.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundlerIdentifier": "com.my-list-app",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.maxi1602.mylistapp",
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./assets/lista.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "CAMERA",
        "ACCESS_FINE_LOCATION",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/lista.png"
    },
    "extra": {
      "eas": {
        "projectId": "e2676f30-bb07-4b85-81cb-d31af60b9572"
      }
    }
  }
}
